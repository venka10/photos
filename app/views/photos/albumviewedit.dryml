	<heading:><%= "Album: #{@album.name}" %></heading:>
	<content-body: replace>
		<a action="new" to="&model" />      
        <page-nav />
		<form action="#{photo_albumviewupdate_path(@album.id)}" without-new-button >
			
        <collection/>
		
